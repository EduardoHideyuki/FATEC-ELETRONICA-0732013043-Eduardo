#include <stdio.h>

int main(void) {
  //definicao de variaveis
  float e_metros_por_segundo,s_kilometros_por_hora;

  printf("Entre com o valor em metros por segundo[m/s]:\n");
  scanf("%f", &e_metros_por_segundo);

  s_kilometros_por_hora = 3.6 * e_metros_por_segundo;

  printf("O valor em kilometros por hora[km/h] Ã©: %.1f\n", s_kilometros_por_hora);

  return 0;
}
